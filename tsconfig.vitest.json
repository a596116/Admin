{
  "extends": "./tsconfig.app.json",
  "exclude": [],
  "compilerOptions": {
    "composite": true,
    "lib": [],
    "types": ["node", "jsdom"],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "paths": {
      "@/*": ["./src/*"],
      "#/*": ["./src/assets/*"]
    }
  },
  "include": [
    "types/module.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "vite.config.ts",
    "vite/**/*.ts",
    "types/**/*.d.ts",
    "types/**/*.ts",
    "types/components.d.ts",
    "src/**/*.json"
  ]
}
