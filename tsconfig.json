{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.node.json"
    },
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.vitest.json"
    }
  ],
  "include": [
    "types/module.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "vite.config.ts",
    "vite/**/*.ts",
    "types/**/*.d.ts",
    "types/**/*.ts",
    "types/components.d.ts",
    "src/**/*.json"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "#/*": ["./src/assets/*"]
    },
    "composite": true,
    "types": ["node"],
    "resolveJsonModule": true,
    "esModuleInterop": true
    // "module": "es2022"
  },
  "types": ["element-plus/global", "lodash-es"]
}
